  "private": true,
  "workspaces": [
    "packages/*",
    "packages/core/*",
    "packages/components/*",
    "packages/react-tools/*",
    "packages/styles/*",
    "tools/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*",
      "packages/core/*",
      "packages/components/*",
      "packages/react-tools/*",
      "packages/styles/*",
      "tools/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wallace5796/wallace-ui.git"
  },
  "scripts": {
    "pkg": "manypkg run",
    "pkg:clean": "manypkg exec rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --watch",
    "build": "yarn build:babel && yarn build:cli && preconstruct build",
    "build:babel": "yarn pkg babel-plugin build",
    "build:cli": "yarn pkg cli build",
    "build:tokens": "wallace-cli tokens packages/styles/theme"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
