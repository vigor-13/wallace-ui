    "@storybook/cli": "^6.4.0",
    "@storybook/react": "^6.4.1",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "add": "^2.0.6",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
