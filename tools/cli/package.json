    "commander": "^8.3.0",
    "ora": "^5.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@types/cli-welcome": "^2.2.0",
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "^5.1.0"
