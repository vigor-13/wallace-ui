{
  "name": "@wallace-ui/cli",
  "version": "0.1.0",
  "description": "Generate Typescript types to provide autocomplete for your custom theme.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
